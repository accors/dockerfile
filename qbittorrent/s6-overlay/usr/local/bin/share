is_ssl=$(grep -i "WebUI\\\HTTPS\\\Enabled=" /data/config/qBittorrent.conf | awk -F "=" '{print $2}')

if [[ $is_ssl == true ]]; then
    cmd_curl="curl -sk"
    url_prefix="https://"
else
    cmd_curl="curl -s"
    url_prefix="http://"
fi

cmd_curl_post="$cmd_curl -X POST"
api_url_base="${url_prefix}127.0.0.1:${WEBUI_PORT}/api/v2"
